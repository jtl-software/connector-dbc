{
  "name": "jtl/connector-dbc",
  "description": "A database API for connectors.",
  "type": "project",
  "authors": [
    {
      "name": "Immanuel Klinkenberg",
      "email": "immanuel.klinkenberg@jtl-software.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@gitlab.jtl-software.de:jtl-software-gmbh/jtlconnector.git",
      "reference": "origin/master"
    }
  ],
  "require": {
    "doctrine/dbal": "^2.5",
    "jtl/connector": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "jtl\\Connector\\CDBC\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "jtl\\Connector\\CDBC\\": "tests/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "phpunit/dbunit": "^3.0",
    "phpunit/php-code-coverage": "^5.0"
  }
}
